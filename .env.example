# HealthMama AI Environment Variables
# Copy this file to .env and update with your actual values

# OpenAI Configuration (REQUIRED)
OPENAI_API_KEY=your_openai_api_key_here

# Flask Configuration (REQUIRED)
FLASK_SECRET_KEY=your_secret_key_here_generate_a_random_string

# Environment Settings
FLASK_ENV=development
PORT=5000
LOG_LEVEL=INFO

# Optional: Rate Limiting
RATELIMIT_STORAGE_URL=memory://

# Optional: Custom Data Paths
# DIABETES_DATA_PATH=data_diabetes/cleaned_diabetes.txt
# PREECLAMPSIA_DATA_PATH=data_preelampsia/cleaned_preecampsia.txt